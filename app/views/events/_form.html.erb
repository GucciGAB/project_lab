
<%= form_with model: @event do |form| %>
    <div>
      <%= form.label :title %><br>
      <%= form.text_field :title %>
      
    </div>
  
    <div>
      <%= form.label :content %><br>
      <%= form.text_area :content %><br>
    </div>

    <div>
    <%= form.label :location %><br>
    <%= form.text_field :location %><br>
    </div>

    <div>
    <%= form.label :start_date %><br>
    <%= form.datetime_field :start_date %><br>
    </div>

    <div>
    <%= form.label :end_date %><br>
    <%= form.datetime_field :end_date %><br>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
  
  <% end %>
